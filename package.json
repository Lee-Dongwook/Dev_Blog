{
  "name": "dongdev-monorepo",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "preview": "turbo run preview",
    "blog:dev": "turbo run dev --filter=@dongdev/blog",

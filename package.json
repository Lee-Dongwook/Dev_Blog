    "about:start": "turbo run start --filter=@dongdev/about"
  },
  "dependencies": {},
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.12",
    "@types/node": "^24.3.0",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@8.15.0"
